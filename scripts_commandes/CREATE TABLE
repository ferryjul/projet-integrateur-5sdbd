DROP DATABASE mydb;
CREATE DATABASE mydb;
USE mydb;

CREATE TABLE data
(
	trip_id VARCHAR,
	trip_duration INT,
	start_time TIMESTAMP,
	stop_time TIMESTAMP,
	start_station_id INT,
	start_station_name VARCHAR,
	start_station_latitude FLOAT,
	start_station_longitude FLOAT,
	end_station_id INT,
	end_station_name VARCHAR,
	end_station_latitude FLOAT,
	end_station_longitude FLOAT,
	bike_id INT,
	user_type VARCHAR,
	birth_year INT,
	gender INT,
	PRIMARY KEY (trip_id)
);

SELECT COUNT(*) FROM data;

CREATE TABLE data ( trip_id VARCHAR, trip_duration INT, start_time TIMESTAMP, stop_time TIMESTAMP, start_station_id INT, start_station_name VARCHAR, start_station_latitude FLOAT, start_station_longitude FLOAT, end_station_id INT, end_station_name VARCHAR, end_station_latitude FLOAT, end_station_longitude FLOAT, bike_id INT, user_type VARCHAR, birth_year INT, gender INT, PRIMARY KEY (trip_id) );